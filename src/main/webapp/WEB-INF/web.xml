<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SaveNoteServlet</servlet-name>
  	<display-name>SaveNoteServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlet.SaveNoteServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>DeleteServlets</servlet-name>
  	<display-name>DeleteServlets</display-name>
  	<description></description>
  	<servlet-class>com.servlet.DeleteServlets</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EditServlet</servlet-name>
  	<display-name>EditServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlet.EditServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SaveNoteServlet</servlet-name>
  	<url-pattern>/SaveNoteServlet</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
  	<servlet-name>DeleteServlets</servlet-name>
  	<url-pattern>/DeleteServlets</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>EditServlet</servlet-name>
    	<url-pattern>/EditServlet</url-pattern>
    </servlet-mapping>
</web-app>
